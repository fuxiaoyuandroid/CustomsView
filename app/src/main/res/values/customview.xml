<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StepView">
        <attr name="outerColor" format="color"></attr>
        <attr name="innerColor" format="color"></attr>

        <attr name="stepTextColor" format="color"></attr>
        <attr name="stepTextSize" format="dimension"></attr>

        <attr name="borderWidth" format="dimension"></attr>
    </declare-styleable>


    <declare-styleable name="ColorChangeTextView">
        <attr name="originalColor" format="color"/>
        <attr name="changeColor" format="color"/>
    </declare-styleable>


    <declare-styleable name="CustomProgressBar">
        <attr name="inBackground" format="color"/>
        <attr name="outBackground" format="color"/>
        <attr name="roundWidth" format="dimension"/>
        <attr name="barTextSize" format="dimension"/>
        <attr name="barTextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="CustomRatingBar">
        <attr name="starNormal" format="reference"/>
        <attr name="starFocus" format="reference"/>
        <attr name="starCount" format="integer"/>
    </declare-styleable>

    <declare-styleable name="LetterSearchBar">
        <attr name="letterSize" format="dimension"/>
        <attr name="letterColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="KgSlidingMenu">
        <attr name="menuRightMargin" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="QqSlidingMenu">
        <attr name="rightPadding" format="dimension"/>
    </declare-styleable>
</resources>